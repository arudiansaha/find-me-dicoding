{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "import/no-extraneous-dependencies": "off",
        "max-len": [
            "error",
            {
                "code": 80,
                "ignoreStrings": true,
                "ignoreTemplateLiterals": true
            }
        ]
    }
}